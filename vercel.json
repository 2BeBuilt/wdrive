{
    "version": 2,
    "builds": [
      {
        "src": "express/app.js",
        "use": "@vercel/node"
      },
      {
        "src": "next/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "express/app.js"
      },
      {
        "src": "/(.*)",
        "dest": "next/$1"
      }
    ]
  }